/* combination/pages/backHome/index.wxss */
.submit {
	width: 100%;
	height: 100vh;
	background-color: #fff;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 9;
	display: flex;
	align-items: center;
	justify-content: center;
	flex-direction: column;
}

.submit-view {
	width: 100%;
	height: auto;
	display: flex;
	align-items: center;
	justify-content: center;
	flex-direction: column;
}

.submit-image {
	width: 200rpx;
	height: 200rpx;
	display: block;
}

.submit-title {
	width: 100%;
	font-size: 36rpx;
	font-weight: 500;
	color: #333333;
	text-align: center;
	margin-top: 48rpx;
}

.submit-back {
	width: 272rpx;
	height: 80rpx;
	border-radius: 44rpx;
	border: 2rpx solid #999999;
	display: flex;
	align-items: center;
	justify-content: center;
	font-size: 32rpx;
	color: #666666;
	text-align: center;
	margin-top: 88rpx;
}