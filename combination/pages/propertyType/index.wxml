	<view class="chengeData">
		<van-tag wx:if="{{ show.primary }}" style="margin-top:20rpx;margin-right:20rpx;" closeable size="medium" size="large" plain id="primary" bind:close="onClose">电梯房</van-tag>
		<van-tag wx:if="{{ show.primary }}" style="margin-top:20rpx;margin-right:20rpx;" closeable size="medium" size="large" plain id="primary" bind:close="onClose">3室</van-tag>
		<van-tag wx:if="{{ show.primary }}" style="margin-top:20rpx;margin-right:20rpx;" closeable size="medium" size="large" plain id="primary" bind:close="onClose">20000-30000</van-tag>
		<van-tag wx:if="{{ show.primary }}" style="margin-top:20rpx;margin-right:20rpx;" closeable size="medium" size="large" plain id="primary" bind:close="onClose">3室</van-tag>
	</view>

	<scroll-view class="list" refresher-enabled="true" scroll-into-view="{{toView}}" refresher-triggered="{{triggered}}" bindrefresherrefresh="topList" bindscrolltolower="scrollList" scroll-y="true">
		<block>
			<shopItem item="{{item}}"></shopItem>
		</block>
	</scroll-view>
</view>