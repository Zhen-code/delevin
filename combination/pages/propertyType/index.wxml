		<!-- 户型 -->
		<view class="typeTreeSelect" wx:if="{{selectType === 2}}">
			<view class="rentalMethod">
				<view class="rentalMethod-name">
					<text class="rentalMethod-leftName">户型</text>
					<text class="rentalMethod-rightName" bindtap="changeReset">重置</text>
				</view>
				<view class="rentalMethod-content">
					<block wx:for="{{type}}" wx:key="index">
						<text class="rentalMethod-content-index {{item.checked?'index-action':''}}"
							data-item="{{item}}" data-index="{{index}}" bindtap="oncheckTab">{{item.title}}</text>
					</block>
				</view>
				<view class="rentalMethod-tap">
					<text class="rentalMethod-tap-text {{conditionIndex === 0?'rentalMethod-tap-text-action':''}}"
						data-index="{{0}}" bindtap="rentTab">不限条件</text>
					<text class="rentalMethod-tap-text {{conditionIndex === 1?'rentalMethod-tap-text-action':''}}"
						data-index="{{1}}" bindtap="rentTab">筛选房源</text>
				</view>
			</view>
			<view class="rentalMethod-hide" bindtap="setHide">
			</view>
		</view>

		<!-- 销售状态 -->
		<view class="typeTreeSelect" wx:if="{{selectType === 3  && title === '新房房源'}}">
			<view class="rentalMethod">
				<view class="rentalMethod-name">
					<text class="rentalMethod-leftName">销售状态</text>
					<text class="rentalMethod-rightName" bindtap="changeReset">重置</text>
				</view>
				<view class="rentalMethod-content">
					<text class="rentalMethod-content-index {{entireRentIndex === 0?'index-action':''}}" data-item="{{'ON_SALE'}}" data-index="{{0}}"
						bindtap="entireRentTab">在售</text>
					<text class="rentalMethod-content-index {{entireRentIndex === 1?'index-action':''}}" data-item="{{'FOR_SALE'}}" data-index="{{1}}"
						bindtap="entireRentTab">待售</text>
				</view>
				<view class="rentalMethod-tap">
					<text class="rentalMethod-tap-text {{conditionIndex === 0?'rentalMethod-tap-text-action':''}}"
						data-index="{{0}}" bindtap="conditionTab">不限条件</text>
					<text class="rentalMethod-tap-text {{conditionIndex === 1?'rentalMethod-tap-text-action':''}}"
						data-index="{{1}}" bindtap="conditionTab">筛选房源</text>
				</view>
			</view>
			<view class="rentalMethod-hide" bindtap="setHide">
			</view>
		</view>

		<!-- 楼龄 -->
		<view class="typeTreeSelect" wx:if="{{selectType === 3 && (title === '二手房房源' || title === '小区房源')}}">
			<view class="rentalMethod">
				<view class="rentalMethod-name">
					<text class="rentalMethod-leftName">楼龄</text>
					<text class="rentalMethod-rightName" bindtap="changeReset">重置</text>
				</view>
				<view class="rentalMethod-content">
					<text class="rentalMethod-content-index {{entireRentIndex === 0?'index-action':''}}" data-index="{{0}}"
						data-item="{{'WITHIN_FIVE_YEARS'}}" bindtap="buildingAgeOptionsTab">5年以内</text>
					<text class="rentalMethod-content-index {{entireRentIndex === 1?'index-action':''}}" data-index="{{1}}"
					data-item="{{'WITHIN_TEN_YEARS'}}" bindtap="buildingAgeOptionsTab">10年以内</text>
					<text class="rentalMethod-content-index {{entireRentIndex === 2?'index-action':''}}" data-index="{{2}}"
					data-item="{{'WITHIN_FIFTEEN_YEARS,'}}" bindtap="buildingAgeOptionsTab">15年以内</text>
					<text class="rentalMethod-content-index {{entireRentIndex === 3?'index-action':''}}" data-index="{{3}}"
					data-item="{{'WITHIN_TWENTY_YEARS'}}" bindtap="buildingAgeOptionsTab">20年以内</text>
					<text class="rentalMethod-content-index {{entireRentIndex === 4?'index-action':''}}" data-index="{{4}}"
					data-item="{{'MORE_TWENTY_YEARS'}}" bindtap="buildingAgeOptionsTab">20年或以上</text>
				</view>
				<view class="rentalMethod-tap">
					<text class="rentalMethod-tap-text {{conditionIndex === 0?'rentalMethod-tap-text-action':''}}"
						data-index="{{0}}" bindtap="conditionTab">不限条件</text>
					<text class="rentalMethod-tap-text {{conditionIndex === 1?'rentalMethod-tap-text-action':''}}"
						data-index="{{1}}" bindtap="conditionTab">筛选房源</text>
				</view>
			</view>
			<view class="rentalMethod-hide" bindtap="setHide">
			</view>
		</view>

		<!-- 出租方式 -->
		<view class="typeTreeSelect" wx:if="{{selectType === 3 && title === '租房房源'}}">
			<view class="rentalMethod">
				<view class="rentalMethod-name">
					<text class="rentalMethod-leftName">出租方式</text>
					<text class="rentalMethod-rightName" bindtap="changeReset">重置</text>
				</view>
				<view class="rentalMethod-content">
					<text class="rentalMethod-content-index {{entireRentIndex === 0?'index-action':''}}" data-item="{{'COTENANCY'}}" data-index="{{0}}"
						bindtap="entireRentTab">整租</text>
					<text class="rentalMethod-content-index {{entireRentIndex === 1?'index-action':''}}" data-item="{{'WHOLE_RENT'}}" data-index="{{1}}"
						bindtap="entireRentTab">合租</text>
				</view>
				<view class="rentalMethod-tap">
					<text class="rentalMethod-tap-text {{conditionIndex === 0?'rentalMethod-tap-text-action':''}}"
						data-index="{{0}}" bindtap="conditionTab">不限条件</text>
					<text class="rentalMethod-tap-text {{conditionIndex === 1?'rentalMethod-tap-text-action':''}}"
						data-index="{{1}}" bindtap="conditionTab">筛选房源</text>
				</view>
			</view>
			<view class="rentalMethod-hide" bindtap="setHide">
			</view>
		</view>
	</view>

	<scroll-view class="list" wx:if="{{item.length !== 0}}" refresher-enabled="true" scroll-into-view="{{toView}}"
		refresher-triggered="{{triggered}}" bindrefresherrefresh="topList" bindscrolltolower="scrollList" scroll-y="true">
		<block wx:for="{{item}}" wx:key="index">
			<shopItem item="{{item}}" data-item="{{item}}" bindtap="toListingDetails"></shopItem>
		</block>
	</scroll-view>
	<onData wx:if="{{item.length === 0}}" height="{{100}}"></onData>
</view>