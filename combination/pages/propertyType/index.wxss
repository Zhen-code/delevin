/* combination/pages/propertyType/index.wxss */
.view {
	width: 100%;
	height: 100%;
	background-color: white;
	display: flex;
	align-items: center;
	justify-content: flex-start;
	flex-direction: column;
	box-sizing: border-box;
	position: fixed;
	left: 0;
	/* z-index: -1; */
}

.citySearch {
	width: 100%;
	height: auto;
	display: flex;
	flex-direction: row;
}

.selectValue {
	width: 100%;
	height: auto;
}

.selectValueAction{
	height: 100vh;
	background-color: rgba(0,0,0,0.4);
}

.view-checked {
	width: 100%;
	height: auto;
	padding: 20rpx 32rpx;
	box-sizing: border-box;
	display: flex;
	align-items: flex-start;
	justify-content: flex-start;
	flex-direction: row;
	flex-wrap: wrap;
}

.type {
	width: 100%;
	height: 88rpx;
	display: flex;
	align-items: center;
	justify-content: center;
	flex-direction: row;
	border-bottom: 1rpx solid #EEEEEE;
	position: relative;
}

.type-index {
	flex: 1;
	height: 100%;
	display: flex;
	align-items: center;
	justify-content: center;
	flex-direction: row;
}

.type-index-name {
	font-size: 28rpx;
	font-weight: 400;
	color: #666666;
}

.type-index-name-action {
	font-size: 28rpx;
	font-weight: bold;
	color: #333333
}

.type-index-icon {
	width: 16rpx;
	height: 16rpx;
	margin-left: 8rpx;
}

.city {
	margin-top: 0 !important;
	padding: 0 32rpx;
	box-sizing: border-box;
}

.typeTreeSelect {
	width: 100%;
	height: 100vh;
	position: absolute;
	top: 90rpx;
	left: 0;
	z-index: 999;
	background: rgba(0, 0, 0, 0.5);
}

.typeTreeSelect-list {
	width: 100%;
	min-height: auto;
	max-height: calc(40vh + 120rpx);
	background-color: white;
	display: flex;
	align-items: flex-start;
	justify-content: flex-start;
	flex-direction: column;
}

.typeTreeSelect-list-row {
	width: 100%;
	height: auto;
	display: flex;
	align-items: flex-start;
	justify-content: flex-start;
	flex-direction: row;
	overflow: hidden;
}

.typeTreeSelect-list-left {
	width: 200rpx;
	height: 40vh;
	background-color: #F8F8F8;
	display: flex;
	align-items: flex-start;
	justify-content: flex-start;
	flex-direction: column;
}

.typeTreeSelect-list-left-index {
	width: 100%;
	height: 100rpx;
	display: flex;
	align-items: center;
	justify-content: center;
	font-size: 28rpx;
	font-weight: 400;
	color: #666666;
}

.left-index-action {
	font-weight: bold;
	color: #333333;
	background-color: #FFFFFF;
	position: relative;
}

.left-index-action::before {
	content: '';
	width: 8rpx;
	height: 40rpx;
	background: #FFD854;
	border-radius: 4rpx;
	position: absolute;
	top: 50%;
	left: 7rpx;
	transform: translate(-50%, -50%);
}

.typeTreeSelect-list-right {
	width: 100%;
	height: 40vh;
	background-color: white;
	display: flex;
	align-items: flex-start;
	justify-content: flex-start;
	flex-direction: row;
}

.typeTreeSelect-list-right-one {
	flex: 1;
	height: 100%;
	display: flex;
	align-items: flex-start;
	justify-content: flex-start;
	flex-direction: column;
	overflow: scroll;
}

.typeTreeSelect-list-right-one:nth-child(1) {
	border-right: 1rpx solid #EEEEEE;
}

.typeTreeSelect-list-right-one-list {
	width: 100%;
	height: auto;
	padding: 0 48rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
	flex-direction: row;
	box-sizing: border-box;
}

.typeTreeSelect-list-right-one-title {
	flex: 1;
	height: 100rpx;
	display: flex;
	align-items: center;
	justify-content: flex-start;
	font-size: 28rpx;
	font-weight: 400;
	color: #666666;
}

.right-one-action {
	font-weight: bold;
	color: #333333;
}


.rentalMethod {
	width: 100%;
	height: auto;
	padding: 0 32rpx;
	box-sizing: border-box;
	display: flex;
	align-content: flex-start;
	justify-content: flex-start;
	flex-direction: column;
	background-color: white;
	z-index: 2;
}

.rentalMethod-hide {
	width: 100%;
	height: 50vh;
	z-index: 1;
}

.rentalMethod-name {
	width: 100%;
	height: 42rpx;
	display: flex;
	align-content: center;
	justify-content: space-between;
	flex-direction: row;
	margin-top: 26rpx;
}

.rentalMethod-leftName {
	font-size: 30rx;
	font-weight: bold;
	color: #333333;
}

.rentalMethod-rightName {
	font-size: 30rx;
	color: #4077FF;
}

.rentalMethod-content {
	width: 100%;
	height: auto;
	display: flex;
	align-content: flex-start;
	justify-content: flex-start;
	flex-wrap: wrap;
	flex-direction: row;
	margin-top: 32rpx;
}

.rentalMethod-content-index {
	width: 214rpx;
	height: 72rpx;
	background: #F8F8F8;
	border-radius: 4rpx;
	border: 1px solid #F8F8F8;
	font-size: 28rpx;
	font-weight: 400;
	color: #666666;
	display: flex;
	align-items: center;
	justify-content: center;
	flex-wrap: wrap;
	flex-direction: row;
	box-sizing: border-box;
}

.rentalMethod-content-index:nth-child(2),
.rentalMethod-content-index:nth-child(5),
.rentalMethod-content-index:nth-child(8),
.rentalMethod-content-index:nth-child(11) {
	margin: 0 20rpx 20rpx 20rpx;
}

.index-action {
	width: 214rpx;
	height: 72rpx;
	background: #FFF9E5;
	border-radius: 2px;
	font-size: 28rpx;
	font-weight: bold;
	color: #333333;
	border: 1px solid #FFD854;
}

.rentalMethod-tap {
	width: 100%;
	height: 120rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
	flex-direction: row;
	margin-top: 32rpx;
	border-top: 1rpx solid #EEEEEE;
}

.rentalMethod-tap-text {
	flex: 1;
	height: 72rpx;
	background: #F8F8F8;
	border-radius: 32rpx;
	display: flex;
	align-items: center;
	justify-content: center;
	font-size: 32rpx;
	font-family: PingFangSC-Regular, PingFang SC;
	font-weight: 400;
	color: #666666;
}

.rentalMethod-tap-text:nth-child(1) {
	margin-right: 32rpx;
}

.rentalMethod-tap-text-action {
	background: #FFD854;
	font-weight: bold;
	color: #333333;
}

.rentalMethod-ding {
	width: 100%;
	height: 88rpx;
	display: flex;
	align-items: center;
	justify-content: flex-start;
	flex-direction: row;
	margin-top: 24rpx;
}

.rentalMethod-ding-text {
	font-size: 28rpx;
	font-weight: 400;
	color: #333333;
	margin-right: 24rpx;
}

.rentalMethod-ding-input {
	flex: 1;
	height: 100%;
	display: flex;
	align-items: center;
	justify-content: flex-start;
	flex-direction: row;
}

.rentalMethod-ding-input input {
	width: 215rpx;
	height: 72rpx;
	padding: 0 24rpx;
	border-radius: 4px;
	border: 1px solid #DDDDDD;
	display: flex;
	align-items: center;
	justify-content: center;
	font-size: 28rpx;
	font-weight: 400;
	color: #333333;
}

.rentalMethod-ding-input text {
	font-size: 24rpx;
	margin: 0 12rpx;
	color: #999999;
}

.chengeData {
	width: 100%;
	height: auto;
	padding: 0 32rpx;
	box-sizing: border-box;
	display: flex;
	flex-wrap: wrap;
	align-items: center;
	flex-direction: row;
	justify-content: flex-start;
}

.list {
	flex: 1;
	height: auto;
	padding: 0 32rpx;
	/* margin-bottom: 100px; */
	overflow: scroll;
	display: flex;
	align-items: flex-start;
	justify-content: flex-start;
	flex-direction: column;
	box-sizing: border-box;
	padding-bottom: 175rpx;
}

::-webkit-scrollbar {
	width: 0;
	height: 0;
	color: transparent;
}