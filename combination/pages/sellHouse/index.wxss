/* combination/pages/sellHouse/index.wxss */
.view {
	width: 100%;
	height: auto;
	background-color: white;
	display: flex;
	align-items: flex-start;
	justify-content: flex-start;
	flex-direction: column;
}

.view-title {
	width: 100%;
	padding: 0 32rpx;
	box-sizing: border-box;
	padding-top: 32rpx;
	text-align: left;
	font-size: 32rpx;
	font-weight: bold;
	color: #333333;
	border-top: 20rpx solid #eeeeee;
}

.view-row {
	width: 100%;
	height: auto;
	padding: 0 0 0 32rpx;
	margin-top: 16rpx;
	box-sizing: border-box;
	display: flex;
	align-items: flex-start;
	justify-content: flex-start;
	flex-direction: column;
}

.view-row-content {
	width: 100%;
	height: auto;
	display: flex;
	align-items: center;
	justify-content: space-between;
	flex-direction: row;
	border-bottom: 1rpx solid #eeeeee;
}

.view-row-content-name {
	width: auto;
	height: 100%;
	display: flex;
	align-items: center;
	justify-content: flex-start;
	color: #666666;
	font-size: 30rpx;
}

.view-row-content-input {
	flex: 1;
	height: auto;
	display: flex;
	align-items: center;
	justify-content: flex-end;
	font-size: 30rpx;
	font-weight: 400;
	color: #333333;
	padding: 0 32rpx;
}

.view-row-content-input-text {
	flex: 1;
	height: 100rpx;
	font-size: 30rpx;
	text-align: right;
	font-size: 30rpx;
	font-weight: 400;
	color: #333333;
}

.view-row-content-input-set {
	width: 4rpx;
	height: 36rpx;
	background-color: #EEEEEE;
	margin: 0 14rpx
}

.view-row-content-input-code {
	width: auto;
	height: 100%;
	display: flex;
	align-items: center;
	justify-content: center;
	font-size: 30rpx;
	font-weight: 400;
	color: #4077FF;
}


.bottom-phone {
	width: 100%;
	height: 70px;
	margin-top: 100rpx;
	/* position: fixed;
	left: 0;
	right: 0;
	bottom: 0; */
	background: #FFFFFF;
}

.bottom-phone-text {
	flex: 1;
	height: 40px;
	margin: 0 16px;
	background: #FFD854;
	border-radius: 22px;
	font-size: 16px;
	font-weight: bold;
	color: #333333;
	display: flex;
	align-items: center;
	justify-content: center;
	margin-top: 10px;
}

.submit {
	width: 100%;
	height: 100vh;
	background-color: #fff;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 9;
	display: flex;
	align-items: center;
	justify-content: center;
	flex-direction: column;
}

.submit-view {
	width: 100%;
	height: auto;
	display: flex;
	align-items: center;
	justify-content: center;
	flex-direction: column;
}

.submit-image {
	width: 200rpx;
	height: 200rpx;
	display: block;
}

.submit-title {
	width: 100%;
	font-size: 36rpx;
	font-weight: 500;
	color: #333333;
	text-align: center;
	margin-top: 48rpx;
}

.submit-back{
	width: 70%;
	height: 80rpx;
	border: 1rpx solid #979797;
	display: flex;
	align-items: center;
	justify-content: center;
	font-size: 24rpx;
	color: #999999;
	margin-top: 200rpx;
	border-radius: 60rpx;
}