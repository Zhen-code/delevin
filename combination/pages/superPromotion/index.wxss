	background-color: white;
	border-top: 1rpx solid #eeeeee;
	display: flex;
	align-items: center;
	justify-content: space-between;
	flex-direction: row;
	position: fixed;
	bottom: 0;
	left: 0;
	right: 0;
	z-index: 999;
}

.footerAll {
	font-size: 36rpx;
	font-weight: 400;
	color: #333333;
	display: flex;
	align-items: center;
	justify-content: flex-start;
	flex-direction: row;
}

.footerPrice {
	font-size: 36rpx;
	font-weight: 500;
	color: #FE6300;
}

.footerShop {
	width: 170px;
	height: 44px;
	background-color: #FFD854;
	border-radius: 22px;
}

.footerShopSurpe {
	width: 100%;
	height: 100%;
	font-size: 32rpx;
	font-weight: bold;
	color: #755132;
