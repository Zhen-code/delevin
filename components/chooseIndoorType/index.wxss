.sheetWrap{
    width: 100%;
    height: 576rpx;
    margin-bottom: 56rpx;
}
.wrap-top{
    height: 454rpx;
    padding: 32rpx;
    box-sizing: border-box;
}
.wrap-top-item{
    padding: 16rpx 32rpx;
    box-sizing: border-box;
    font-size: 28rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: #666666;
    background: #F8F8F8;
    display: inline-block;
    margin: 0 24rpx 24rpx 0;
}
.active{
    background: #FFF9E5;
    border-radius: 4rpx;
    border: 2rpx solid #FFD854;
    font-size: 28rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: #333333;
}

.wrap-bottom{
    width: 100%;
    height: 120rpx;
    padding: 24rpx 32rpx 66rpx 32rpx;
    box-sizing:border-box;
    display: flex;
    justify-content: space-between;
}
.clear{
    width: 332rpx;
    height: 72rpx;
    background: #F8F8F8;
    border-radius: 36rpx;
    font-size: 32rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: #666666;
    display: flex;
    justify-content: center;
    align-items: center;
}
.sure{
    width: 332rpx;
    height: 72rpx;
    background: #FFD854;
    border-radius: 36rpx;
    font-size: 32rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    color: #333333;
    display: flex;
    align-items: center;
    justify-content: center;
}
