/* pages/home/components/seach.wxss */
/* @import '../../../../assets/iconfont/iconfont.wxss'; */
.shade {
  background: rgba(0, 0, 0, 0.5);
}

.content {
  background-color: white;
  box-sizing: border-box;
  /* position: fixed; */
  /* top: 0;
  left: 0;
  right: 0; */
  z-index: 99;
}

.search {
  display: flex;
  height: 80rpx;
  line-height: 80rpx;
  align-items: center;
  padding: 30rpx 25rpx 0 25rpx;
}

.search-border {
  display: flex;
  width: 614rpx;
  height: 80rpx;
  /* line-height: 80rpx; */
  align-items: center;
  border: 1rpx solid rgba(229, 229, 229, 1);
  box-shadow: 0px 0px 8px 0px rgba(0, 0, 0, 0.05);
  border-radius: 8rpx;
}

.search-cancel {
  width: 50rpx;
  height: 50rpx;
  margin-right: 10rpx;
  /* position: absolute; */
}

.search-line {
  width: 1rpx;
  height: 48rpx;
  background: rgba(229, 229, 229, 1);
}

.area {
  width: 150rpx;
  display: flex;
  align-items: center;
  height: 78rpx;
  padding-left: 25rpx;
  font-size: 34rpx;
  color: #000;
  font-weight: 500;
}

.area image {
  width: 14rpx;
  height: 10rpx;
  margin-left: 18rpx;
}

.pull {
  /* width:0;
  height:0;
  display: inline-block; */
  margin-left: 18rpx;
  /* border-top:20rpx solid #231815;
	border-right:14rpx solid transparent;
	border-left:14rpx solid transparent; */
}

.search input {
  flex: 1;
  height: 78rpx;
  padding-left: 24rpx;
  font-size: 28rpx;
  color: #999;
}

.icon-tel {
  width: 60rpx;
  /* height: 40rpx; */
  margin-left: 27rpx;
  font-size: 60rpx;
  /* background-color: #EDEDED; */
}

.icon-search {
  width: 28rpx;
  height: 26rpx;
  margin-left: 32rpx;
  /* font-size: 35rpx; */
  /* background-color: #EDEDED; */
}

.tel-img {
  width: 60rpx;
  height: 60rpx;
  margin-left: 27rpx;
}

.search-img {
  width: 28rpx;
  height: 26rpx;
  margin-left: 32rpx;
}

.condition {
  width: 100%;
  height: auto;
  text-align: center;
  border-bottom: 1rpx solid #eee;
}

.condition-line {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.condition-item {
  width: 25%;
  height: 80rpx;
  color: #666666;
  font-size: 28rpx;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
}

.condition-item image {
  width: 18rpx;
  height: 18rpx;
  margin-left: 10rpx;
}

.selected {
  display: flex;
  width: 100%;
  flex-direction: row;
  padding: 0 10rpx;
  flex-wrap: wrap;
  color: #333333;
  font-size: 28rpx;
  font-weight: 400;

  margin-bottom: 32rpx;
}

.selected span {
  height: 58rpx;
  margin-left: 50rpx;
  line-height: 58rpx;
  margin-top: 22rpx;
}

.selected view {
  height: 58rpx;
  margin-top: 22rpx;
  padding: 10rpx 12rpx 8rpx 16rpx;
  background-color: #F8F8F8;
  margin-left: 16rpx;
  box-sizing: border-box;
  display: inline-flex;
  flex-direction: row;
  align-items: center;
  border-radius: 8rpx;
}


.selected image {
  width: 24rpx;
  height: 24rpx;
}

.scroll-box {
  width: 100%;
  height: 680rpx;
}

.scroll-content {
  width: 100%;
  display: flex;
  flex-direction: row;
}

.scroll-first {
  width: 187rpx;
  height: 590rpx;
  display: flex;
  flex-direction: column;
  background-color: #f8f8f8;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.scroll-first text {
  display: block;
  text-align: center;
  font-size: 28rpx;
  /* font-weight: 400; */
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
}

.scroll-second {
  flex: 1;
  display: flex;
  height: 590rpx;
  flex-direction: column;
  /* padding-bottom: 20px; */
  overflow: hidden;
  /* box-sizing: border-box; */
}

.scroll-second text {
  font-weight: 400;
  height: 70rpx;
  font-size: 28rpx;
  display: block;
  padding-left: 64rpx;
  line-height: 70rpx;
}

.square-content {
  display: flex;
  font-size: 28rpx;
  color: #333333;
  flex-direction: row;
  justify-content: space-between;
  flex-wrap: wrap;
  padding: 0rpx 40rpx 40rpx 40rpx;
  /* margin-top:24rpx; */
}

.square-content text {
  margin-top: 24rpx;
  width: 208rpx;
  height: 70rpx;
  line-height: 70rpx;
  background-color: #f8f8f8;
  text-align: center;
  border-radius: 8rpx;
}

.square-self {
  height: 80rpx;
  width: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-direction: row;
  flex-wrap: wrap;
  margin-bottom: 32rpx;
}

.square-self span {
  height: 80rpx;
  line-height: 80rpx;
  margin-left: 13rpx;
  margin-right: 14rpx;
}

.square-self text {
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  font-weight: 500;
  margin-left: 32rpx;
  padding-right: 18rpx;
  text-align: center;
  color: #333333;
}

.square-self input {
  width: 244rpx;
  height: 72rpx;
  border-radius: 8rpx;
  border: 2rpx solid #DDDDDD;
  text-align: left;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  padding: 0 16rpx;
  font-size: 28rpx;
  color: #333333;
}

.free-self {
  padding-left: 64rpx;
  /* height: 80rpx; */
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin-top: 20rpx;
  box-sizing: border-box;
  /* margin-bottom: 42rpx; */
  /* padding-bottom: 42rpx; */
}

.free-self::after {
  content: "";
  width: 100%;
  height: 20px;
}

.free-self text {
  height: 70rpx;
  line-height: 70rpx;
  font-size: 30rpx;
  font-weight: 400;
  padding-left: 0;
  width: 100%;
  text-align: left;
  color: #333;
}

.rentalMethod-name {
	width: 100%;
  height: 42rpx;
  padding: 0 32rpx;
  box-sizing: border-box;
	display: flex;
	align-content: center;
	justify-content: space-between;
	flex-direction: row;
	margin-top: 26rpx;
}

.rentalMethod-leftName {
	font-size: 30rx;
	font-weight: bold;
	color: #333333;
}

.rentalMethod-rightName {
	font-size: 30rx;
	color: #4077FF;
}

.free-self span {
  height: 80rpx;
  line-height: 80rpx;
  margin-left: 13rpx;
  margin-right: 14rpx;
}

.free-self input {
  width: 150rpx;
  height: 80rpx;
  border: 1rpx solid #e5e5e5;
  text-align: center;
  box-sizing: border-box;
}

.left64 {
  margin-left: 64rpx;
}

.screen {
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  height: 1000rpx;
  /* padding-bottom: 100rpx; */
}

.screen-item {
  width: 100%;
  padding: 0 40rpx 40rpx 40rpx;
  display: flex;
  /* height: 300rpx; */
  flex-direction: row;
  box-sizing: border-box;
  justify-content: space-between;
  flex-wrap: wrap;
}

.screen-item-title {
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
}

.screen-item text {
  margin-top: 24rpx;
  width: 208rpx;
  height: 70rpx;
  line-height: 70rpx;
  background-color: #f8f8f8;
  text-align: center;
  border-radius: 8rpx;
  align-self: flex-start;
}

.screen-item:after {
  content: "";
  width: 208rpx;
  height: 70rpx;
  display: block;
  visibility: hidden;
}

.buttom-group {
  width: 100%;
  height: 90rpx;
  display: flex;
  flex-direction: row;
  border-top: 1rpx solid #eee;
  color: #333;
}

.buttom-group .button {
  width: 385rpx;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 30rpx;
  text-align: center;
  font-weight: 600;
}

.buttom-group .button:first-child {
  border-right: 1rpx solid #eee;
}

.bgcolor-white {
  background-color: #fff;
  color: #333333;
  font-weight: 600;
}

.bgcolor-red {
  background-color: #FFEBEC !important;
  border: 1rpx solid rgba(255, 48, 58, 1);
  border-radius: 8rpx;
  box-sizing: border-box
}

.color-red {
  color: #333333;
  font-weight: bold;
}

.color-ff303a {
  color: #ff303a;
}

.rentalMethod-tap {
  width: 100%;
  height: 120rpx;
  padding: 0 32rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  flex-direction: row;
  border-top: 1rpx solid #EEEEEE;
}

.rentalMethod-tap-text {
  flex: 1;
  height: 72rpx;
  background: #F8F8F8;
  border-radius: 32rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #666666;
}

.rentalMethod-tap-text:nth-child(1) {
  margin-right: 32rpx;
}

.rentalMethod-tap-text-action {
  background: #FFD854;
  font-weight: bold;
  color: #333333;
}

.rentalMethod-content {
	width: 100%;
  height: auto;
  padding: 0 32rpx;
  box-sizing: border-box;
	display: flex;
	align-content: flex-start;
	justify-content: flex-start;
	flex-wrap: wrap;
	flex-direction: row;
	margin-top: 32rpx;
}

.rentalMethod-content-index {
	width: 214rpx;
	height: 72rpx;
	background: #F8F8F8;
	border-radius: 4rpx;
	border: 1px solid #F8F8F8;
	font-size: 28rpx;
	font-weight: 400;
	color: #666666;
	display: flex;
	align-items: center;
	justify-content: center;
	flex-wrap: wrap;
	flex-direction: row;
	box-sizing: border-box;
}

.rentalMethod-content-index:nth-child(2),
.rentalMethod-content-index:nth-child(5),
.rentalMethod-content-index:nth-child(8),
.rentalMethod-content-index:nth-child(11) {
	margin: 0 20rpx 20rpx 20rpx;
}

.index-action {
	width: 214rpx;
	height: 72rpx;
	background: #FFF9E5;
	border-radius: 2px;
	font-size: 28rpx;
	font-weight: bold;
	color: #333333;
	border: 1px solid #FFD854;
}
