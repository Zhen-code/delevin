/* pages/personal/home/index.wxss */
.view {
	width: 100%;
	height: 100%;
	background-color: #ffffff;
}

.bgColor{
	background: rgba(248, 248, 248, 1);
}

.content {
	width: 100%;
	min-height: 100vh;
	max-height: 100%;
	background-size: 100% 360rpx;
	background-repeat: no-repeat;
	background-image: url('http://beiru.oss-cn-hangzhou.aliyuncs.com/admin-file/701b7e0d-9e0b-4b73-b615-9f2b6beb19fc.png');
}

.top {
	width: 100%;
	height: auto;
	padding: 10rpx 32rpx;
	box-sizing: border-box;
	display: flex;
	align-items: center;
	justify-content: center;
	flex-direction: row;
}

.top-left {
	width: auto;
	height: 32rpx;
	display: flex;
	align-items: center;
	justify-content: flex-start;
	flex-direction: row;
	margin-right: 24rpx;
}

.top-left-title {
	font-size: 30rpx;
	font-weight: bold;
	color: rgba(51, 51, 51, 1);
	margin-right: 10rpx;
}

.top-left-border {
	height: 0;
	border-left: 8rpx solid transparent;
	border-right: 8rpx solid transparent;
	border-top: 8rpx solid #475669;
}

.top-right {
	flex: 1;
	height: 68rpx;
	border-radius: 34rpx;
	background-color: #ffffff;
	display: flex;
	align-items: center;
	justify-content: flex-start;
	flex-direction: row;
}

.top-right-image {
	width: 50rpx;
	height: 50rpx;
	display: block;
	margin: 0 8rpx 0 20rpx
}

.top-right-text {
	flex: 1;
	padding-right: 20rpx;
	font-size: 28rpx;
	font-weight: 400;
	color: #999999
}

.view-swiper {
	width: 100%;
	height: auto;
	padding: 0 32rpx;
	box-sizing: border-box;
	border-radius: 8rpx;
	margin-top: 16rpx;
}

.swiper-item {
	width: 100%;
	height: 320rpx;
}

.swiper-item-image{
	width: 100%;
	height: 320rpx;
	display: block;
}

.components {
	width: 100%;
	height: auto;
	padding: 0 32rpx;
	box-sizing: border-box;
}

.components-top {
	width: 100%;
	height: 44rpx;
	margin-top: 40rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
	flex-direction: row;
}

.components-top-left {
	flex: 1;
	height: 100%;
	font-size: 32rpx;
	font-weight: bold;
	color: rgba(51, 51, 51, 1);
}

.components-top-right{
	flex: 1;
	height: 100%;
	font-size: 32rpx;
	font-weight: bold;
	color: rgba(51, 51, 51, 1);
	display: flex;
	justify-content: flex-start;
}

.owner {
	width: 100%;
	height: 152rpx;
	margin-top: 24rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
	flex-direction: row;
}

.owner-left {
	width: 47%;
	padding-left: 48rpx;
	box-sizing: border-box;
	height: 100%;
	display: flex;
	align-items: flex-start;
	justify-content: center;
	flex-direction: column;
	background-position: center center;
	background-size: 100% 100%;
	background-repeat: no-repeat;
	background-image: url('http://beiru.oss-cn-hangzhou.aliyuncs.com/admin-file/16946559-db73-42a9-9d80-f7f5b78256a5.png');
}

.owner-left-title1 {
	font-size: 34rpx;
	font-weight: bold;
	color: rgba(51, 51, 51, 1);
}

.owner-left-title2 {
	font-size: 28rpx;
	font-weight: 400;
	color: rgba(102, 102, 102, 1);
}

.owner-rigth {
	width: 50%;
	height: 100%;
	padding-right: 48rpx;
	box-sizing: border-box;
	display: flex;
	flex-direction: column;
	align-items: flex-start;
	justify-content: center;
	background-size: 100% 100%;
	background-position: center center;
	background-image: url('http://beiru.oss-cn-hangzhou.aliyuncs.com/admin-file/7e715b51-546c-48e0-9233-933d9ef4ee2b.png');
	background-repeat: no-repeat;
	padding-left: 24rpx;
	box-sizing: border-box;
}

.owner-rigth-image {
	width: 192rpx;
	height: 82rpx;
}

.home-message {
	width: 100%;
	height: auto;
	margin: 34rpx 0;
	display: flex;
	align-items: center;
	justify-content: flex-start;
	flex-direction: row;
	background: #F8F8F8;
}

.home-message-image {
	width: 62rpx;
	height: 24rpx;
	margin-left: 24rpx;
}

.home-message-text {
	flex: 1;
	height: 40px;
	display: flex;
	align-items: center;
	justify-content: center;
}

.swiperTitle {
	width: 100%;
	height: 20px;
	display: flex;
	align-items: center;
	justify-content: center;
}

.swiperTitle swiper-item {
	width: 100%;
	height: 100%;
	line-height: 20px;
	padding: 0 24rpx;
	box-sizing: border-box;
	font-size: 28rpx;
	font-weight: 400;
	color: rgba(102, 102, 102, 1);
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 1;
	word-break: break-all;
}

.border {
	border-bottom: 20rpx solid #F8F8F8
}

.home-bottom {
	width: 100%;
	height: 88rpx;
	background: rgba(248, 248, 248, 1);
}

.home-bottom text {
	flex: 1;
	height: 100%;
	font-size: 26rpx;
	font-weight: 400;
	color: rgba(153, 153, 153, 1);
	text-align: center;
	line-height: 100%;
	display: flex;
	align-items: center;
	justify-content: center;
}

.mine-content-top {
	width: 686rpx;
	height: auto;
	padding: 0 40rpx;
	margin: 28rpx 32rpx 56rpx 32rpx;
	background: rgba(255, 255, 255, 1);
	border-radius: 4px;
	display: flex;
	align-items: center;
	justify-content: center;
	flex-direction: column;
	box-sizing: border-box;
}

.mine-content-top-box {
	width: 100%;
	height: 148rpx;
	margin-top: 20rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
	flex-direction: row;
}

.mine-content-top-left,
.mine-content-top-rigth {
	display: flex;
	align-items: center;
	justify-content: center;
	flex-direction: row;
}

.mine-content-top-left-title {
	display: flex;
	/* align-items: center; */
	justify-content: flex-start;
	flex-direction: column;
}

.mine-content-top-left-text {
	font-size: 40rpx;
	font-weight: bold;
	margin-left: 24rpx;
	color: rgba(51, 51, 51, 1)
}

.mine-content-top-rigth-text {
	font-size: 26rpx;
	font-weight: 400;
	color: rgba(64, 119, 255, 1);
}

.mine-content-top-rigth-title {
	font-size: 26rpx;
	font-weight: 400;
	color: #666666;
}

.mine-content-top-rigth-title text {
	font-size: 26rpx;
	font-weight: 400;
	color: #FF4E56;
	margin-left: 10rpx;
}


.mine-content-top-left-type {
	font-size: 30rpx;
	font-weight: 400;
	margin-left: 24rpx;
	color: rgba(102, 102, 102, 1);
}

.introductionText {
	width: 100%;
	height: auto;
	margin: 20rpx 0 38rpx 0;
	font-size: 28rpx;
	font-weight: 400;
	color: rgba(153, 153, 153, 1);
}

.floor {
	width: 100%;
	height: auto;
	padding: 0 32rpx;
	box-sizing: border-box;
	display: flex;
	align-items: flex-start;
	justify-content: flex-start;
	flex-direction: column;
	margin-bottom: 40rpx;
}

.floor-list {
	width: 100%;
	height: auto;
	display: flex;
	align-items: flex-start;
	justify-content: flex-start;
	flex-direction: column;
}

.floor-list-row {
	width: 100%;
	height: auto;
	display: flex;
	align-items: flex-start;
	justify-content: flex-start;
	flex-direction: column;
	margin-bottom: 40rpx;
	position: relative;
}

.view-no-row{
	width: 100%;
	height: 694rpx;
	position: absolute;
	top: 0;
	left: 0;
	z-index: 99;
	overflow: hidden;
}

.view-no-row-top{
	width: 100%;
	height: 440rpx;
	display: flex;
	align-items: center;
	justify-content: center;
	background: linear-gradient(180deg, rgba(0, 0, 0, 0.5) 0%, rgba(0, 0, 0, 0.7) 100%);
}

.view-no-row-top-name {
	width: 200rpx;
	height: 72rpx;
	background: rgba(255, 255, 255, 0.4);
	border-radius: 2rpx;
	border: 2rpx solid #FFFFFF;
	font-size: 30rpx;
	color: #FFFFFF;
	display: flex;
	align-items: center;
	justify-content: center;
}

.view-no-row-no-bottom {
